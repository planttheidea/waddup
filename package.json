{
  "author": "planttheidea",
  "ava": {
    "babel": "inherit",
    "failFast": true,
    "files": [
      "./test/utils.js",
      "./test/index.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "bugs": {
    "url": "https://github.com/planttheidea/waddup/issues"
  },
  "description": "A ridiculously tiny pubsub manager with no dependencies",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel": "6.5.2",
    "babel-cli": "6.10.1",
    "babel-eslint": "6.1.1",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "^6.9.0",
    "ejs": "2.4.2",
    "eslint": "3.0.1",
    "eslint-friendly-formatter": "2.0.5",
    "eslint-loader": "1.4.1",
    "html-webpack-plugin": "^2.22.0",
    "in-publish": "^2.0.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "sinon": "^1.17.4",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "homepage": "https://github.com/planttheidea/waddup#readme",
  "keywords": [
    "publish",
    "subscribe",
    "unsubscribe",
    "pubsub",
    "waddup"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "waddup",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/waddup.git"
  },
  "scripts": {
    "build": "NODE_ENV=development ./node_modules/webpack/bin/webpack.js --progress --colors",
    "build-minified": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --progress --colors --config=webpack.config.minified.js",
    "compile-for-publish": "npm run lint && npm run test && npm run transpile && npm run build && npm run build-minified",
    "dev": "NODE_ENV=development ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors --config=webpack.config.dev.js",
    "lint": "NODE_ENV=test ./node_modules/eslint/bin/eslint.js src",
    "prepublish": "in-publish && npm run compile-for-publish || in-install",
    "test": "NODE_ENV=test ava --no-cache",
    "test:watch": "NODE_ENV=test ava --no-cache --watch",
    "transpile": "./node_modules/babel-cli/lib/babel/index.js -d lib/ src/"
  },
  "version": "1.0.3"
}
